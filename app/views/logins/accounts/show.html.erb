<div id="canvas">
  <div class="centered-panel">
  <fieldset>
    <label for="account_name"><%= t("name") %></label>
    <input type="text" value="<%= @account.name %>" />
    <label for="account_name"><%= t("contact") %></label>
    <input type="text" value="<%= @account.contact %>"/>
    <label for="account_name"><%= t("telephone") %></label>
    <input type="text" value="<%= @account.telephone %>" />
    <label for="account_name"><%= t("email") %></label>
    <input type="text" value="<%= @account.email %>" />
    <button class="success btn accept"><%= t("accept") %></button>
  </fieldset>
    </div>
</div>

<script type="text/javascript">
  $(function() {
    new Trackwane.Controllers.AccountController({el: "#canvas"});
  });
</script>
